{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT\n            id,\n            name,\n            description,\n            project_id,\n            external_id,\n            created_at,\n            updated_at\n        FROM role r\n        WHERE ($1 OR project_id = $2)\n            AND ($3 OR id = any($4))\n            AND ($5 OR name ILIKE ('%' || $6 || '%'))\n            --- PAGINATION\n            AND ((r.created_at > $7 OR $7 IS NULL) OR (r.created_at = $7 AND r.id > $8))\n        ORDER BY r.created_at, r.id ASC\n        LIMIT $9\n        ",
